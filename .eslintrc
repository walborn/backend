{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "curly": [ "error", "multi-line" ],
    "object-curly-newline": ["error", { "consistent": true }],
    "array-bracket-spacing": ["error", "always"],
    "indent": [ 2, 4, { "SwitchCase": 1 } ],
    "comma-dangle": [ "error", "always-multiline" ],
    "max-len": [ "error", 200, 2 ],
    "one-var": [ "error", "never" ]
  }
}
